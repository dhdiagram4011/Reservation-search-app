{
    "builders": [{
        "type": "docker",
        "image": "ubuntu:14.04",
        "export_path": "nacyot-ubuntu-wget.tar"
    }],
    "provisioners": [{
        "type": "shell",
        "inline": [
            "apt-get update",
            "mkdir -p /var/www/html",
            "apt-get install -y wget"
        ]
    }],
    "post-processors": [{
        "type": "docker-import",
        "repository": "build-test",
        "tag": "wget"
    }]
}